Tag - Array

53. Maximum Subarray
class Solution(object):
    def maxSubArray(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        """
        idea: partial sum, but too much runnig time
        """
        # length = len(nums)
        # partial_sum = []
        # element = 0
        # for i in range(length):
        #     element += nums[i]
        #     partial_sum.append(element)
        # sup = max(partial_sum)
        # for i in range(1, length):
        #     prev = partial_sum[0:i]
        #     sup = max(sup, partial_sum[i]-min(prev))
        # return sup
        """
        idea: replace the element in list with maximum accumulated sum to this point
        """
        length = len(nums)
        max_sum = nums[0]
        for i in range(1,length):
            nums[i] = max(nums[i], nums[i]+nums[i-1])
            max_sum = max(nums[i], max_sum)
        return max_sum

